#
# .alias file for VCDEV environment
#
# command aliases
alias grep 'grep -n'
alias word 'egrep ^\!:1$ /usr/dict/words | egrep -v "[\!:2]"'
alias psg 'ps -ef | grep \!* | grep -v grep'
alias so 'source ~/.cshrc'
alias h 'history \!* | tail -39 | more'
alias cp '/bin/cp -i'
alias rm '/bin/rm -i'
alias mv '/bin/mv -i'
alias lla 'ls -laF'
alias ll 'ls -al'
alias la 'lsf -a'
alias l 'ls -al'
alias c 'clear'
alias j 'jobs'
alias bdf 'df -k'
alias delcore '/bin/rm -f core'
alias prcore 'find ~ -name core -print'
alias rmcore 'find ~ -name core -exec rm {} \;'
alias vi 'vim -X'
alias vir 'vim -R'
alias make 'make -j8'
alias mr 'make run'
# local command shortcuts
alias d 'dismp'
alias start 'startmp'
alias stop 'stopmp'
# quick directory change
alias cdh 'cd /home/vc/projects/Universal'
alias cda 'cd /home/vc/projects/Universal/Apps'
alias cdc 'cd /home/vc/projects/Universal/Core'
alias cde 'cd /home/vc/projects/Universal/ExternalLib'
alias cdini 'cd /home/vc/etc/ini'
alias cdl 'cd /home/vc/lib'
alias cdb 'cd /home/vc/bin'
# quick job change
alias j 'jobs'
alias 1 'fg %1'
alias 2 'fg %2'
alias 3 'fg %3'
alias 4 'fg %4'
alias 5 'fg %5'
alias 6 'fg %6'
alias 7 'fg %7'
alias 8 'fg %8'
alias 9 'fg %9'
